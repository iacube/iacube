<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:uxap="sap.uxap" xmlns="sap.m" controllerName="requisitions_report.controller.Requisition"
	xmlns:layout="sap.ui.layout" xmlns:common="iacube.ui.common" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Job Requisition">
		<content>
			<uxap:ObjectPageLayout id="detObj" showTitleInHeaderContent="false">
				<uxap:headerTitle>
					<uxap:ObjectPageHeader objectImageURI="sap-icon://product" objectTitle="{requis>Position}" objectSubtitle="{requis>ReqId}"
						objectImageShape="Circle" isObjectIconAlwaysVisible="true"></uxap:ObjectPageHeader>
				</uxap:headerTitle>
				<uxap:headerContent>
					<layout:VerticalLayout width="5%"></layout:VerticalLayout>
					<layout:VerticalLayout>
						<ObjectStatus title="{i18n>location}" text="{requis>Location}"/>
						<ObjectStatus title="{i18n>project}" text="{requis>Project}"/>
					</layout:VerticalLayout>
					<layout:VerticalLayout width="40%"></layout:VerticalLayout>
					<layout:VerticalLayout>
						<ObjectStatus title="{i18n>created}" text="{requis>CreatedOn}"/>
						<FlexBox alignContent="End" justifyContent="End">
							<ObjectStatus text="{requis>Status}" state="{path: 'requis>Status'}"/>
						</FlexBox>
						<layout:VerticalLayout></layout:VerticalLayout>
						<FlexBox alignContent="End" justifyContent="End">
							<Select></Select>
						</FlexBox>
					</layout:VerticalLayout>
				</uxap:headerContent>
				<uxap:sections>
					<uxap:ObjectPageSection id="detInfo" title="{i18n>info}" titleUppercase="false">
						<uxap:subSections>
							<uxap:ObjectPageSubSection title="">
								<uxap:blocks>
									<common:InfoForm modelName="ui"/>
								</uxap:blocks>
							</uxap:ObjectPageSubSection>
						</uxap:subSections>
					</uxap:ObjectPageSection>
					<uxap:ObjectPageSection id="detSkills" title="{i18n>skills}" titleUppercase="false">
						<uxap:subSections>
							<uxap:ObjectPageSubSection title="{i18n>skills}">
								<uxap:blocks>
									<common:SkillsTable modelName="ui"/>
								</uxap:blocks>
							</uxap:ObjectPageSubSection>
						</uxap:subSections>
					</uxap:ObjectPageSection>
					<!--	<ObjectPageSection id="detCandidates" title="{i18n>candidates}" titleUppercase="false">
						<subSections>
							<ObjectPageSubSection title="">
								<blocks>
									<info:InfoBlock id="cand" columnLayout="2"/>
								</blocks>
							</ObjectPageSubSection>
						</subSections>
					</ObjectPageSection> -->
				</uxap:sections>
			</uxap:ObjectPageLayout>
		</content>
	</Page>
</mvc:View>