<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:uxap="sap.uxap" xmlns="sap.m"
	controllerName="requisitions_report.controller.Requisition"
	xmlns:layout="sap.ui.layout" xmlns:common="iacube.ui.common"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Job Requisition">
		<content>
			<uxap:ObjectPageLayout id="detObj"
				showTitleInHeaderContent="false">
				<uxap:headerTitle>
					<uxap:ObjectPageHeader objectImageURI="sap-icon://product"
						objectTitle="{ui>Title} ({ui>ReqId})">
						<uxap:breadCrumbsLinks>
							<Link text="Job Requisitions Report: by Status" press="handleLink1Press" />
						</uxap:breadCrumbsLinks>
					</uxap:ObjectPageHeader>
				</uxap:headerTitle>
				<uxap:headerContent>
					<layout:VerticalLayout width="5%"></layout:VerticalLayout>
					<layout:VerticalLayout>
						<ObjectStatus title="{i18n>req.detail.location}"
							text="{ui>Location}" />
						<ObjectStatus title="{i18n>req.detail.project}" text="{ui>ProjectId}" />
					</layout:VerticalLayout>
					<layout:VerticalLayout width="40%"></layout:VerticalLayout>
					<layout:VerticalLayout>
						<ObjectStatus title="{i18n>req.detail.created}" text="{ui>CreatedAt}" />
						<FlexBox alignContent="End" justifyContent="End">
							<ObjectStatus text="{ui>StatusCodeId}" state="Success" />
						</FlexBox>
						<layout:VerticalLayout></layout:VerticalLayout>
						<FlexBox alignContent="End" justifyContent="End">
							<Select></Select>
						</FlexBox>
					</layout:VerticalLayout>
				</uxap:headerContent>
				<uxap:sections>
					<uxap:ObjectPageSection id="detInfo"
						title="{i18n>req.detail.info}" titleUppercase="false">
